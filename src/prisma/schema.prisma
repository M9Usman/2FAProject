// src/prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/.prisma/client" // default path; optional
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  name     String
  password String
  role     String
}
